    "babel",
    "babel-plugin",
    "pipeline",
    "pipeline-operator"
  ],
  "files": [
    "LICENSE",
    "README.md",
    "lib/"
  ],
  "dependencies": {
    "babel-plugin-syntax-pipeline": "0.1.0"
  },
  "devDependencies": {
    "babel-cli": "6.24.0",
    "babel-core": "6.24.0",
    "babel-eslint": "7.1.1",
    "babel-preset-es2015": "6.24.0",
    "babel-preset-stage-0": "6.22.0",
    "chai": "3.5.0",
    "eslint": "3.18.0",
    "husky": "0.13.2",
    "istanbul": "0.4.5",
    "mocha": "3.2.0",
    "source-map-support": "0.4.14"
