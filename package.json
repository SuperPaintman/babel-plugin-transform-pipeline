    "prepublish": "npm run lint && npm run build && npm run test",
    "prepush": "npm run prepublish",
    "precommit": "npm run lint"
  },
  "author": {
    "name": "SuperPaintman",
    "email": "SuperPaintmanDeveloper@gmail.com",
    "url": "https://superpaintman.com/"
  },
  "license": "MIT",
