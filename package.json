  "description": "Compile pipeline operator to ES5",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel src -d lib -s",
    "build:watch": "babel src -d lib -s -w",
    "lint": "eslint src",
    "test": "mocha --opts ./mocha.opts ./lib/**/test.js",
    "test:watch": "mocha --opts ./mocha.opts --watch ./lib/**/test.js",
